AdvancedAnalyticsUrl_shortenerShortened_urlsResultTable = (
		site_id as number) =>
    let
        site_id = if site_id = null then "" else "&site_id=" & Uri.EscapeDataString(Text.From(site_id)),
parameters = Text.TrimStart(Text.Combine({ site_id, "&page_size=100" }), {"&"}),
        requestUrl = "https://api.siteimprove.com/v2/analytics/url_shortener/shortened_urls?" & parameters
    in
        Table.GenerateByPage((previous) => 
            let
                nextLink = if (previous = null) then requestUrl else Value.Metadata(previous)[NextLink]?,
                page = if (nextLink <> null) then AdvancedAnalyticsUrl_shortenerShortened_urlsResultTablePage(nextLink) else null
            in
                page
        );

AdvancedAnalyticsUrl_shortenerShortened_urlsParamsType = type function (
	site_id as (type number meta [
        Documentation.FieldCaption = "site_id",
        Documentation.FieldDescription = "Id for specific site."
    ]))
    as table meta [
        Documentation.Name = "Get all account-specific shortened URLs"
    ];

AdvancedAnalyticsUrl_shortenerShortened_urlsResultType = type table [
	id = number, short_url = text, source_url = text, url_created_date = text
];
  
AdvancedAnalyticsUrl_shortenerShortened_urlsResultTablePage = (url as text) as table =>
    let
        json = Json.Document(Web.Contents(url, SiteimproveRequestOptions)),
        nextLink = GetNextLink(json),
        itemsTable = JsonToTable(json),
        fields = Record.FieldNames(Type.RecordFields(Type.TableRow(AdvancedAnalyticsUrl_shortenerShortened_urlsResultType))),
        expandedItemsTable = Table.ExpandRecordColumn(itemsTable, "Column1", fields),
        appliedSchema = Table.ChangeType(expandedItemsTable, AdvancedAnalyticsUrl_shortenerShortened_urlsResultType)
		, addedKey = Table.AddKey(appliedSchema, {"id", "url_created_date"}, true)
